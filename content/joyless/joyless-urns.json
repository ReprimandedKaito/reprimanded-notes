[

{
    "title": "Anime News Network",
    "key": "ann",
    "isUrn": true,
    "pattern": "ann:{type}:{id}",
    "urlTemplate": "www.animenewsnetwork.com/encyclopedia/{type}.php?id={id}",
    "enums": {
        "type": ["anime", "manga"]
    }
},

{
    "title": "IGDB",
    "key": "igbd",
    "pattern": "igdb:game:{idInBase36}",
    "urlTemplate": "www.igdb.com/g/{idInBase36}",
    "description": "EXAMPLE: Baten Kaitos' id is `3795`; in base 36, it is `2xf`; so its URL is www.igdb.com/g/2xf"
},

{
    "title": "IMDb",
    "pattern": "imdb:{id}",
    "urlTemplate": "www.imdb.com/title/{id}"
},

{
    "title": "MangaUpdates",
    "pattern": "mangaupdates:series:{seriesId}",
    "urlTemplate": "api.mangaupdates.com/v1/series/{seriesId}"
},

{
    "title": "MyAnimeList",
    "pattern": "mal:{type}:{id}",
    "urlTemplate": "myanimelist.net/{type}/{id}",
    "enums": {
        "type": ["anime", "manga"]
    }
},

{
    "title": "Spacetoon Go",
    "pattern": "spacetoongo:series:{id}",
    "urlTemplate": "https://spacetoongo.com/series/{id}"
},

{
    "title": "Steam",
    "pattern": "steam:{id}",
    "urlTemplate": "store.steampowered.com/app/{id}"
},


{
    "title": "Vocaloid Database",
    "pattern": "vocadb:{songId}",
    "urlTemplate": "vocadb.net/S/{songId}"
},

{
    "title": "VNDB",
    "pattern": "vndb:{id}",
    "urlTemplate": "vndb.org/{id}"
},

{
    "title": "YouTube",
    "pattern": "youtube:{id}",
    "urlTemplate": "www.youtube.com/watch?v={id}"
}

]
